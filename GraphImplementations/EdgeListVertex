
/**
 * Represents a node in an edge list graph structure that can store
 * an item of data.
 * 
 * @author Panchama
 * @version May 2017
 */
public class Vertex
{
    private Object data;
    
    public Vertex(Object data) {
        this.data = data;
    }
    
    public Object getData() {
        return data;
    }
    
    public void setData(Object data) {
        this.data = data;
    }
}
