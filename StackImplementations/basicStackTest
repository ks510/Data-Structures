
/**
 * Basic Test class for Stack implementations
 * @author Karen Suen
 * @version March 2017
 */
public class TestClass
{
    private Stack s1;

    /**
     * Constructor for objects of class TestClass
     */
    public TestClass()
    {
        s1 = new Stack();
    }

    /**
     * Add n amount of integer elements to top of stack
     */
    public void pushTest(int n) {
        for(int i = 0; i < n; i++) {
            s1.push(i);
        }
    }

    /**
     * Remove n amount of elements from the top of stack
     */
    public void popTest(int n) {
        for(int i = 0; i < n; i++) {
            s1.pop();
        }
    }
}
